{
  "plugins": [
    "import",
    "promise",
    "react",
    "security",
    "@typescript-eslint"
  ],
  "extends": [
    "plugin:react/recommended",
    "plugin:security/recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "settings": {
    "react": {
      "pragma": "React",
      "version": "detect"
    },
    "import/parsers": {
      "@typescript-eslint/parser": [
        ".ts",
        ".tsx"
      ]
    },
    "import/resolver": {
      "node": {
        "extensions": [
          ".js",
          ".jsx",
          ".ts",
          ".tsx"
        ]
      }
    }
  },
  "overrides": [
    {
      "files": [
        "**/*.tsx"
      ],
      "rules": {
        "react/prop-types": 0,
        "import/named": 0
      }
    },
    {
      "files": [
        "server/**/*.js",
        "scripts/**/*.js",
        "webpack.*.js"
      ],
      "rules": {
        "no-console": 0
      }
    },
    {
      "files": [
        "**/*.jsx",
        "**/*.js"
      ],
      "rules": {
        "@typescript-eslint/explicit-function-return-type": 0,
        "@typescript-eslint/explicit-module-boundary-types": 0,
        "@typescript-eslint/no-var-requires": 0
      }
    },
    {
      "files": [
        "**/*.tsx",
        "**/*.ts"
      ],
      "rules": {
        "no-shadow": "off",
        "no-undef": "off",
        "no-use-before-define": "off",
        "@typescript-eslint/ban-types": "off"
      }
    },
    {
      "files": [
        "**/*test.tsx",
        "**/*.test.ts"
      ],
      "rules": {
        "@typescript-eslint/no-var-requires": "off"
      }
    }
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "node": true,
    "browser": true,
    "builtin": true,
    "es6": true
  },
  "rules": {
    "block-scoped-var": 2,
    "camelcase": [
      2,
      {
        "properties": "always"
      }
    ],
    "curly": [
      2,
      "all"
    ],
    "dot-notation": [
      2,
      {
        "allowKeywords": true
      }
    ],
    "eqeqeq": [
      2,
      "allow-null"
    ],
    "padding-line-between-statements": [
      2,
      {
        "blankLine": "always",
        "prev": "var",
        "next": "return"
      },
      {
        "blankLine": "always",
        "prev": "block-like",
        "next": "*"
      }
    ],
    "guard-for-in": 2,
    "no-script-url": 0,
    "new-cap": 2,
    "no-bitwise": 0,
    "no-caller": 2,
    "no-cond-assign": [
      2,
      "except-parens"
    ],
    "no-debugger": 2,
    "no-empty": 2,
    "no-eval": 2,
    "no-duplicate-imports": 2,
    "no-param-reassign": 2,
    "no-multi-assign": 2,
    "no-extend-native": 2,
    "no-extra-parens": 2,
    "no-dupe-class-members": 2,
    "no-unneeded-ternary": 2,
    "no-else-return": 2,
    "spaced-comment": 2,
    "no-useless-constructor": 2,
    "no-useless-concat": 2,
    "no-irregular-whitespace": 2,
    "no-iterator": 2,
    "no-loop-func": 2,
    "no-multi-str": 2,
    "no-new": 2,
    "no-var": 2,
    "no-plusplus": 0,
    "no-proto": 2,
    "no-sequences": 2,
    "no-shadow": 2,
    "no-undef": 2,
    "no-unused-vars": 2,
    "no-with": 2,
    "no-extra-semi": 2,
    "object-shorthand": 2,
    "prefer-destructuring": [
      2,
      {
        "AssignmentExpression": {
          "array": false
        }
      }
    ],
    "prefer-const": [
      2,
      {
        "destructuring": "all"
      }
    ],
    "arrow-body-style": 2,
    "semi": [
      2,
      "always"
    ],
    "operator-linebreak": [
      2,
      "after"
    ],
    "valid-typeof": 2,
    "wrap-iife": [
      2,
      "inside"
    ],
    "no-use-before-define": [
      0,
      {
        "functions": false
      }
    ],
    "no-unused-expressions": 2,
    "complexity": [
      2,
      10
    ],
    "max-depth": [
      2,
      4
    ],
    "max-params": [
      2,
      5
    ],
    "max-statements": [
      2,
      15
    ],
    "space-before-function-paren": [
      2,
      {
        "anonymous": "ignore",
        "named": "never"
      }
    ],
    "no-spaced-func": 2,
    "array-bracket-spacing": [
      2,
      "never",
      {}
    ],
    "space-in-parens": [
      2,
      "never"
    ],
    "quote-props": [
      2,
      "as-needed"
    ],
    "key-spacing": [
      2,
      {
        "beforeColon": false,
        "afterColon": true
      }
    ],
    "space-unary-ops": [
      2,
      {
        "words": true,
        "nonwords": false
      }
    ],
    "no-mixed-spaces-and-tabs": 2,
    "no-console": 1,
    "comma-dangle": [
      2,
      "never"
    ],
    "no-trailing-spaces": 2,
    "yoda": [
      2,
      "never"
    ],
    "brace-style": [
      2,
      "1tbs",
      {
        "allowSingleLine": true
      }
    ],
    "no-multiple-empty-lines": [
      2,
      {
        "max": 1
      }
    ],
    "one-var": [
      2,
      "never"
    ],
    "comma-spacing": [
      2,
      {
        "before": false,
        "after": true
      }
    ],
    "semi-spacing": [
      2,
      {
        "before": false,
        "after": true
      }
    ],
    "space-before-blocks": [
      2,
      "always"
    ],
    "comma-style": [
      2,
      "last"
    ],
    "space-infix-ops": 2,
    "eol-last": 2,
    "keyword-spacing": [
      2,
      {}
    ],
    "lines-around-comment": [
      2,
      {
        "beforeLineComment": true,
        "allowBlockStart": true
      }
    ],
    "max-len": [
      2,
      120
    ],
    "consistent-this": [
      2,
      "self"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "quotes": [
      2,
      "single",
      {
        "avoidEscape": true
      }
    ],
    "jsx-quotes": [
      2,
      "prefer-double"
    ],
    "indent": [
      2,
      "tab",
      {
        "SwitchCase": 1
      }
    ],
    "no-multi-spaces": 2,
    "prefer-arrow-callback": 2,
    "arrow-parens": 2,
    "react/no-danger": 2,
    "react/jsx-no-bind": 2,
    "react/jsx-key": 2,
    "react/jsx-pascal-case": 2,
    "react/jsx-curly-spacing": 2,
    "react/jsx-max-props-per-line": 2,
    "react/jsx-handler-names": 2,
    "react/self-closing-comp": 2,
    "react/sort-comp": 2,
    "react/require-default-props": 2,
    "react/require-optimization": 2,
    "security/detect-object-injection": 0,
    "promise/always-return": 2,
    "promise/no-return-wrap": 2,
    "promise/param-names": 2,
    "promise/catch-or-return": 2,
    "promise/no-nesting": 2,
    "promise/avoid-new": 0,
    "import/named": 2,
    "import/no-unresolved": 2,
    "import/no-mutable-exports": 2,
    "import/newline-after-import": 2,
    "import/no-deprecated": 2,
    "import/order": [
      1,
      {
        "groups": [
          "builtin",
          "external",
          "internal",
          "parent",
          "sibling",
          "index"
        ]
      }
    ],
    "import/no-cycle": [
      1,
      {
        "maxDepth": 2
      }
    ],
    "import/first": [
      2,
      "absolute-first"
    ],
    "@typescript-eslint/camelcase": 0,
    "security/detect-non-literal-fs-filename": 0
  }
}
